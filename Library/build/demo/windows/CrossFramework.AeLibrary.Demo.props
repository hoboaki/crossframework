  </ItemDefinitionGroup>
  <Import Project="$(VCTargetsPath)\Microsoft.Cpp.targets" />
  <ImportGroup Label="ExtensionTargets">
  </ImportGroup>
  <ItemGroup>
    <ResFilesImpl Include="@(ResFiles)" Exclude="$(ProjectDir)\..\res\**\.svn\**" />
  </ItemGroup>
  <Target Name="AfterBuild">
    <Copy
